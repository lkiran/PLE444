{% layout "layout-boxed" %}
{% block title %}Boxed Layout{% endblock %}

{% block breadcrumb %}
<li><a href="index.html">Home</a></li>
<li><a href="#">Layout</a></li>
<li class="active"><a href="layout-boxed.html">Boxed Layout</a></li>
{% endblock %}

{% block styles %}
{% endblock %}

{% block scripts %}
{% endblock %}

{% block content %} 
<div data-widget-group="group1">
	<div class="row">
		<div class="col-md-12">

			<div class="panel panel-default" data-widget='{"draggable": "false"}'>
				<div class="panel-heading">
					<h2>Backgrounds for Boxed Layout</h2>
					<div class="panel-ctrls" data-actions-container="" data-action-collapse='{"target": ".panel-body"}'></div>
				</div>
				<div class="panel-body">
					<h4>Background Colors and Patterns</h4>
					<div class="demo-blocks color-dark"></div>
					<div class="demo-blocks color-red"></div>
					<div class="demo-blocks color-green"></div>
					<div class="demo-blocks color-blue"></div>
					<div class="demo-blocks color-lavender"></div>
					<div class="demo-blocks pattern-brickwall"></div>
					<div class="demo-blocks pattern-dark-stripes"></div>
					<div class="demo-blocks pattern-rockywall"></div>
					<div class="demo-blocks pattern-subtle-carbon"></div>
					<div class="demo-blocks pattern-tweed"></div>
					<div class="demo-blocks pattern-vertical-cloth"></div>
					<div class="demo-blocks pattern-grey_wash_wall"></div>
					<div class="demo-blocks pattern-pw_maze_black"></div>
					<div class="demo-blocks pattern-wild_oliva"></div>
					<div class="demo-blocks pattern-stressed_linen"></div>
					<div class="demo-blocks pattern-sos"></div>
				</div>
			</div>

		</div>
	</div>
</div>
{% endblock %}