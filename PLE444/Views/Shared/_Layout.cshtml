<!DOCTYPE html>
<html lang="tr">
<head>
	<meta charset="utf-8">
    <title>PLE - @ViewBag.Title</title>
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0, user-scalable=no">
	<meta name="apple-mobile-web-app-capable" content="yes">
	<meta name="apple-touch-fullscreen" content="yes">
	<meta name="description" content="Description Here">
	<meta name="author" content="Author Name Here">
	<link type='text/css' href='http://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400italic,600' rel='stylesheet'>
	@Styles.Render("~/Content/fonts/font-awesome/css/font-awesome.min.css") <!-- Font Awesome -->
	@Styles.Render("~/Content/fonts/themify-icons/themify-icons.css") <!-- Themify Icons -->
	@Styles.Render("~/Content/css/styles.css") <!-- Core CSS with all styles -->
	@Styles.Render("~/Content/plugins/codeprettifier/prettify.css") <!-- Code Prettifier -->
	@Styles.Render("~/Content/plugins/iCheck/skins/minimal/blue.css") <!-- iCheck -->     
    @Styles.Render("~/Content/css/jquery.filer.css")
    @Styles.Render("~/Content/css/jquery.filer-dragdropbox-theme.css")
    @Styles.Render("~/Content/plugins/summernote/dist/summernote.css") 
    @Styles.Render("~/Content/plugins/switchery/switchery.css")
    @RenderSection("styles", required: false)
    
   
    
		   
	<!--[if lt IE 10]>
		<script type="text/javascript" src="assets/js/media.match.min.js"></script>
		<script type="text/javascript" src="assets/js/respond.min.js"></script>
		<script type="text/javascript" src="assets/js/placeholder.min.js"></script>
	<![endif]-->
	<!-- The following CSS are included as plugins and can be removed if unused-->

</head>
<body class="animated-content">

	<header id="topnav" class="navbar navbar-default navbar-fixed-top navbar-indigo" role="banner">
		<div class="logo-area">
			<span id="trigger-sidebar" class="toolbar-trigger toolbar-icon-bg">
				<a data-toggle="tooltips" data-placement="right" title="Toggle Sidebar">
					<span class="icon-bg">
						<i class="ti ti-menu"></i>
					</span>
				</a>
			</span>

		    <a title="Ana Sayfa" class="toolbar-icon-bg" href="@Url.Action("Index","Home")"><h3 style="color: white; line-height: 25px;">PLE</h3></a>
			<div class="toolbar-icon-bg hidden-xs" id="toolbar-search">
				<div class="input-group">
					<span class="input-group-btn"><button class="btn" type="button"><i class="ti ti-search"></i></button></span>
					<input type="text" class="form-control" placeholder="Grup, Soru veya Ders ara...">
					<span class="input-group-btn"><button class="btn" type="button"><i class="ti ti-close"></i></button></span>
				</div>
			</div>
		</div><!-- logo-area -->
		<ul class="nav navbar-nav toolbar pull-right">
			<li class="toolbar-icon-bg visible-xs-block" id="trigger-toolbar-search">
				<a href="#"><span class="icon-bg"><i class="ti ti-search"></i></span></a>
			</li>

			<li class="toolbar-icon-bg hidden-xs" id="trigger-fullscreen">
				<a href="#" alt="Tam Ekran" class="toggle-fullscreen"><span class="icon-bg"><i class="ti ti-fullscreen"></i></span></a>
			</li>

			@{ Html.RenderAction("LogedInUser", "Layout"); }
		</ul>
	</header>
	<div id="wrapper">
		<div id="layout-static">
			<div class="static-sidebar-wrapper sidebar-midnightblue">
				<div class="static-sidebar">
					<div class="sidebar">
						<div class="widget">
						</div>
						<div class="widget" id="widget-sidebar">
							<nav role="navigation" class="widget-body">
								<ul class="acc-menu">
								    <li>
								        <a href="@Url.Action("List","Course")"><i class="ti ti-direction"></i><span>Dersler</span></a> 
								        <ul class="" style="display: block;">
								            @{ Html.RenderAction("Courses", "Layout"); }
											
								        </ul>
								    </li>
                                    <li>
										<a href="#_"><i class="ti ti-direction"></i><span>Alanlar</span></a>
										<ul class="" style="display: block;">
                                            @{ Html.RenderAction("Spaces", "Layout"); }
										</ul>
									</li>
									<li>
										<a href="@Url.Action("Index","Community", new { id = "" })"><i class="ti ti-direction"></i><span>Gruplar</span></a>
										<ul class="" style="display: block;">
                                            @{ Html.RenderAction("Communities", "Layout"); }
										</ul>
									</li>
									
								</ul>
							</nav>
						</div>
					</div>
				</div>
			</div>
			<div class="static-content-wrapper">
				<div class="static-content">
					<div class="page-content">
                        <ol class="breadcrumb">
                            <li class="active"><a href="#"></a></li>
                        </ol>
                        <div class="container-fluid">
                            <div class="row">
                                <div class="col-md-9">                                
                                    @RenderBody()
                                </div>
                                @*<div class="col-md-3">
                                    <div class="panel panel-default" data-widget='{"draggable": "false"}'>
                                        <div class="panel-heading">
                                            <h2>Panel</h2>
                                            <div class="panel-ctrls" data-actions-container="" data-action-collapse='{"target": ".panel-body"}'></div>
                                        </div>
                                        <div class="panel-body">
                                            Asperiores porro eveniet debitis quas sed harum nobis libero voluptatibus dolorum odio at veniam aut id corrupti hic amet consectetur adipisicing tenetur ex ea dignissimos volupta elit esse quisquam fugiat.
                                        </div>
                                    </div>
                                </div>*@
                            </div>
                        </div>
					</div> <!-- #page-content -->
				</div>
				<footer role="contentinfo">
					<div class="clearfix">
						<ul class="list-unstyled list-inline pull-left">
						    <li><small><em>Boğaziçi University - CET <span style="margin: 0;">&copy; @DateTime.Now.Year</span></em></small></li>
						</ul>
						<button class="pull-right btn btn-link btn-xs hidden-print" id="back-to-top"><i class="ti ti-arrow-up"></i></button>
					</div>
				</footer>
			</div>
		</div>
	</div>

	<!-- Load site level scripts -->
	<!-- <script src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
	<script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.9.2/jquery-ui.min.js"></script> -->
	@Scripts.Render("~/Content/js/jquery-1.10.2.min.js")<!-- Load jQuery -->
	@Scripts.Render("~/Content/js/jqueryui-1.10.3.min.js")	<!-- Load jQueryUI -->
	@Scripts.Render("~/Content/js/bootstrap.min.js")<!-- Load Bootstrap -->
	@Scripts.Render("~/Content/js/enquire.min.js")<!-- Load Enquire -->
	@Scripts.Render("~/Content/plugins/velocityjs/velocity.min.js")	<!-- Load Velocity for Animated Content -->
	@Scripts.Render("~/Content/plugins/velocityjs/velocity.ui.min.js")
	@Scripts.Render("~/Content/plugins/wijets/wijets.js")<!-- Wijet -->
	@Scripts.Render("~/Content/plugins/codeprettifier/prettify.js")<!-- Code Prettifier  -->
	@Scripts.Render("~/Content/plugins/bootstrap-switch/bootstrap-switch.js")<!-- Swith/Toggle Button -->
	@Scripts.Render("~/Content/plugins/bootstrap-tabdrop/js/bootstrap-tabdrop.js")<!-- Bootstrap Tabdrop -->
	@Scripts.Render("~/Content/plugins/iCheck/icheck.min.js")<!-- iCheck -->
	@Scripts.Render("~/Content/plugins/nanoScroller/js/jquery.nanoscroller.min.js")<!-- nano scroller -->
	@Scripts.Render("~/Content/js/application.js")
	@Scripts.Render("~/Content/demo/demo.js")
	@Scripts.Render("~/Content/demo/demo-switcher.js")
    @Scripts.Render("~/Content/js/jquery.filer.js")
    @Scripts.Render("~/Content/plugins/summernote/dist/summernote.min.js")
    @Scripts.Render("~/Content/plugins/switchery/switchery.js")
    @Scripts.Render("~/Scripts/bootbox.js")
	<!-- End loading site level scripts -->
	<!-- Load page level scripts-->
	<!-- End loading page level scripts-->
	@RenderSection("scripts", required: false)
</body>
</html>